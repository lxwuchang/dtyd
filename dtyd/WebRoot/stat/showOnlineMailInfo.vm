<script type="text/javascript"> 
  var area_hash = new Hashtable();
    $(document).ready(function(){
  
   aCity = areaData.split(",");
    for (i = 1; i <= aCity.length; i++) {

	area_hash.put(aCity[i - 1].substring(0, 6), aCity[i - 1].substring(7, aCity[i - 1].length));

  }
     set_form_key(); 
    
