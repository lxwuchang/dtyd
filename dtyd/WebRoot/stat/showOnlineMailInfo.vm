function set_form_key(){

	var ipt = document.getElementsByTagName("td");

	for(i=0;i<9;i++){

		if(ipt[i].className=="wltz"){

			var bm=ipt[i].id;

			var mc=area_hash.get(bm);

			ipt[i].innerText=mc;

		}

	}

}
